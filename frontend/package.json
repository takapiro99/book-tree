{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3004",
        "clean": "rimraf ./.next",
        "dash": "npm run clean && npm run dev",
        "build": "next build",
        "start": "next start",
        "lint": "yarn lint:eslint && yarn lint:prettier",
        "lint:eslint": "next lint",
        "lint:prettier": "prettier --check --ignore-path .gitignore \"./**/*.{js,jsx,ts,tsx,json,scss}\"",
        "format": "prettier --write --ignore-path .gitignore \"./**/*.{js,jsx,ts,tsx,json,css,scss}\""
    },
    "dependencies": {
        "next": "11.0.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-icons": "^4.2.0",
        "sanitize.css": "^12.0.1"
    },
    "devDependencies": {
        "@types/react": "17.0.14",
        "eslint": "7.31.0",
        "eslint-config-next": "11.0.1",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "sass": "^1.38.0",
        "sass-loader": "^12.1.0",
        "typescript": "4.3.5"
    }
}
